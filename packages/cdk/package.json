{
  "name": "@dot/cdk",
  "version": "2.0.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "A set of tools and conventions for working with AWS CDK",
  "license": "MPL-2.0",
  "repository": "shellscape/dot",
  "author": "shellscape",
  "homepage": "https://github.com/shellscape/dot",
  "bugs": "https://github.com/shellscape/dot/issues",
  "main": "dist/index.js",
  "bin": {
    "deploy": "bin/deploy"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "pnpm -w package:build $PWD",
    "prepare": "if [ ! -d 'dist' ]; then pnpm build; fi",
    "prepublishOnly": "pnpm build",
    "prerelease": "pnpm build",
    "release": "pnpm --workspace-root package:release $npm_package_name"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "peerDependencies": {
    "@swc-node/register": "^1.5.4",
    "@swc/core": "^1.3.5",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "@aws-cdk/aws-kinesisfirehose-alpha": "2.61.1-alpha.0",
    "@aws-cdk/aws-kinesisfirehose-destinations-alpha": "2.61.1-alpha.0",
    "@dot/env": "workspace:*",
    "@dot/log": "workspace:*",
    "aws-cdk-lib": "^2.61.1",
    "aws-sdk": "^2.1012.0",
    "camelcase": "^6.2.0",
    "chalk": "^4.1.0",
    "constructs": "^10.1.229",
    "nanoid": "3.3.4",
    "source-map-support": "^0.5.21",
    "yargs-parser": "^21.1.1"
  },
  "devDependencies": {
    "@types/source-map-support": "^0.5.6",
    "@types/yargs-parser": "^21.0.0"
  }
}
